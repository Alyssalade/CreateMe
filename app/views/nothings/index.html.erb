<p id="notice"><%= notice %></p>

<h1>Create Me</h1>

<table>
  <thead>
    <tr>
      <th>Note tes idées de création !</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nothings.each do |nothing| %>
      <tr>
        <td><%= nothing.name %></td>
        <td><%= link_to 'Show', nothing %></td>
        <td><%= link_to 'Edit', edit_nothing_path(nothing) %></td>
        <td><%= link_to 'Destroy', nothing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


